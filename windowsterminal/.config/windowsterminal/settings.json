{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "closeTab"
            },
            "id": "User.closeTab.0"
        }
    ],
    "alwaysShowTabs": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "initialCols": 120,
    "initialRows": 30,
    "keybindings": 
    [
        {
            "id": "Terminal.SplitPaneDown",
            "keys": "alt+shift+minus"
        },
        {
            "id": "Terminal.PrevTab",
            "keys": "ctrl+shift+tab"
        },
        {
            "id": "Terminal.CopyToClipboard",
            "keys": "ctrl+c"
        },
        {
            "id": "User.closeTab.0",
            "keys": "ctrl+shift+w"
        },
        {
            "id": "Terminal.OpenSettingsFile",
            "keys": "ctrl+comma"
        },
        {
            "id": "Terminal.PasteFromClipboard",
            "keys": "ctrl+v"
        },
        {
            "id": "Terminal.FindText",
            "keys": "ctrl+shift+f"
        },
        {
            "id": "Terminal.DecreaseFontSize",
            "keys": "ctrl+minus"
        },
        {
            "id": "Terminal.IncreaseFontSize",
            "keys": "ctrl+plus"
        },
        {
            "id": "Terminal.DuplicatePaneAuto",
            "keys": "alt+shift+d"
        },
        {
            "id": "Terminal.OpenNewTab",
            "keys": "ctrl+shift+t"
        },
        {
            "id": "Terminal.MoveFocusDown",
            "keys": "alt+down"
        },
        {
            "id": "Terminal.SplitPaneRight",
            "keys": "alt+shift+plus"
        },
        {
            "id": "Terminal.MoveFocusLeft",
            "keys": "alt+left"
        },
        {
            "id": "Terminal.DecreaseFontSize",
            "keys": "ctrl+numpad_minus"
        },
        {
            "id": "Terminal.MoveFocusRight",
            "keys": "alt+right"
        },
        {
            "id": "Terminal.MoveFocusUp",
            "keys": "alt+up"
        },
        {
            "id": "Terminal.NextTab",
            "keys": "ctrl+tab"
        },
        {
            "id": "Terminal.ResizePaneDown",
            "keys": "alt+shift+down"
        },
        {
            "id": "Terminal.ResizePaneLeft",
            "keys": "alt+shift+left"
        },
        {
            "id": "Terminal.ResizePaneRight",
            "keys": "alt+shift+right"
        },
        {
            "id": "Terminal.ResizePaneUp",
            "keys": "alt+shift+up"
        },
        {
            "id": "Terminal.SwitchToTab0",
            "keys": "ctrl+1"
        },
        {
            "id": "Terminal.SwitchToTab1",
            "keys": "ctrl+2"
        },
        {
            "id": "Terminal.SwitchToTab2",
            "keys": "ctrl+3"
        },
        {
            "id": "Terminal.SwitchToTab3",
            "keys": "ctrl+4"
        },
        {
            "id": "Terminal.SwitchToTab4",
            "keys": "ctrl+5"
        },
        {
            "id": "Terminal.ScrollDown",
            "keys": "ctrl+shift+down"
        },
        {
            "id": "Terminal.ScrollUp",
            "keys": "ctrl+shift+up"
        },
        {
            "id": "Terminal.ToggleFullscreen",
            "keys": "f11"
        },
        {
            "id": "Terminal.ScrollUpPage",
            "keys": "ctrl+shift+pgup"
        },
        {
            "id": "Terminal.ScrollDownPage",
            "keys": "ctrl+shift+pgdn"
        },
        {
            "id": "Terminal.ToggleFullscreen",
            "keys": "alt+enter"
        },
        {
            "id": "Terminal.IncreaseFontSize",
            "keys": "ctrl+numpad_plus"
        },
        {
            "id": "Terminal.ResetFontSize",
            "keys": "ctrl+0"
        }
    ],
    "launchMode": "default",
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
